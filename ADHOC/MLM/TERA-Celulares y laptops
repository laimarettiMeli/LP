SELECT  
shp.shp_shipment_id,shp.shp_substatus_id,shp.shp_status_id
,shp.SHP_LOGISTIC_CENTER_ID
,shp.SHP_PICKING_TYPE_ID
,shp.SHP_DATE_CREATED_ID
,shp.SHP_DATETIME_CREATED_ID
,shp.SHP_DATE_DELIVERED_ID
,shp.SHP_DATETIME_DELIVERED_ID
,shp.SHP_DATE_HANDLING_ID
,shp.SHP_DATETIME_HANDLING_ID
,shp.SHP_DATE_SHIPPED_ID

,ite.ITE_ITEM_TITLE,ite.ITE_ITEM_ID

--,cat.CAT_CATEG_NAME_L7,cat.CAT_DOMAIN_ID,cat.VERTICAL

,r.SHP_LG_FACILITY_ID,r.SHP_LG_VEHICLE_PLATE_ID,r.SHP_LG_DRIVER_ID,r.SHP_LG_SHIPMENT_STATUS
,r.SHP_LG_SHIPMENT_SUB_STATUS,SHP_LG_SHIPMENT_STATUS_RT,r.SHP_LG_SHIPMENT_SUB_STATUS_RT,r.SHP_LG_ROUTE_STATUS

FROM WHOWNER.BT_SHP_SHIPMENTS shp
  LEFT JOIN WHOWNER.BT_SHP_LG_SHIPMENTS_ROUTES r on r.shp_shipment_id=shp.shp_shipment_id
  INNER JOIN WHOWNER.LK_ITE_ITEMS_PH ite on 
  --cast(ite.cat_categ_id as VARCHAR(100))=cast(shp.cat_categ_id_l7 as VARCHAR(100))
  --  AND (ITE_ITEM_DOM_DOMAIN_ID in('MLM-NOTEBOOKS','MLM-CELLPHONES'))

   INNER JOIN AG_LK_CAT_CATEGORIES_PH cat on (cat.CAT_CATEG_ID_L7=ite.CAT_CATEG_ID_L7)
    and cat.photo_id = 'TODATE' and cat.SIT_SITE_ID='MLM' and ITE_ITEM_DOM_DOMAIN_ID in('MLM-NOTEBOOKS','MLM-CELLPHONES')
 LEFT JOIN BT_SHP_LG_SHIPMENTS lshp on lshp.shp_shipment_id = shp.shp_shipment_id
  INNER JOIN WHOWNER.LK_SHP_SHIPPING_SERVICES SERV ON (SERV.shp_service_id=SHP.shp_service_id) AND SHP_CARRIER_ID_AJUS IN('MERCADOENVIOS','MERCADO ENVIOS','MELI LOGISTICS')

WHERE --shp.shp_shipment_id in (40889027372,40807194403) and
shp.sit_site_id in ('MLM')
AND SHP_LOGISTIC_CENTER_ID in ('MXCD01','MXCD02')
AND shp.SHP_DATETIME_CREATED_ID between current_date -10 and current_date-1
AND shp.SHP_DATE_DELIVERED_ID is null
AND shp.SHP_DATE_SHIPPED_ID is null