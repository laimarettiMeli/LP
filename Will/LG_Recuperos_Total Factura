SELECT 
SHP_COMPANY_NAME as MLP,
SIT_SITE_ID,
SHP_LG_PRE_INVOICE_HEADER_PERIOD_NAME AS Quinzena,
SHP_LG_PRE_INVOICE_DETAIL_TOTAL_COST,
CAST (SHP_LG_PRE_INVOICE_DETAIL_LAST_UPDATED as Date) as LAST_UPDATED,
LEFT (SHP_LG_PRE_INVOICE_DETAIL_DESCRIPTION,POSITION(' ' IN shp_lg_pre_invoice_detail_description)) AS Status,
RIGHT (SHP_LG_PRE_INVOICE_DETAIL_DESCRIPTION,11) AS Shipment

FROM BT_SHP_LG_PRE_INVOICE_DETAIL
WHERE SHP_LG_PRE_INVOICE_DETAIL_TYPE_OPERATION = '+'
GROUP BY 1,2,3,4,5,6,7
